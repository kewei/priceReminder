{
  "manifest_version": 2,

  "name": "PriceReminder Plugin",
  "description": "Reminding the price of your favorite products by one click.",
  "version": "1.1",
  "oauth2": {
    "client_id": "xxxxxxx-xxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/plus.me https://www.googleapis.com/auth/userinfo.email"
    ],
    "aud": "https:\\/\\/accounts.google.com\\/o\\/oauth2\\/token"
  },

  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "Click here!",
    "popup": "add.html"
  },
  "permissions": [
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "https://www.pricereminder.se/*",
    "https://pricereminder.se/*",
    "tabs"
  ],
  "web_accessible_resources": [
  "font-awesome-4.6.3/css/*.*",
  "font-awesome-4.6.3/fonts/*.*"
  ],
  "content_scripts": [

  ]
}
